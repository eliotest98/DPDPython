FileClass -> FromList ImportList VariableList Constructor(Function) FunctionList ClassList

ClassList -> ClassList Class |
             Class |
             /* empty */

Class -> VariableList Constructor(Function) FunctionList

FunctionList -> FunctionList Function |
                Function |
                /* empty */

Function -> ParameterList VariableList Return

ParameterList -> ParameterList PARAM |
                 PARAM |
                 /* empty */

VariableList -> VariableList Var |
                Var |
                /* empty */

Return -> CONST |
          Var |
          CallFunction |
          ITER                                      (For, While)
          /* empty */

Var -> ID CONST |
       ID CallFunction |
       ID

CallFunction -> ID.CallFunction |
                FUN_NAME ( ParameterList ) |
                FUN_NAME ()

FromList -> FromList From |
            From |
            /* empty */

From -> FROM CONST Import

ImportList -> ImportList Import |
              Import |
              /* empty */

Import -> IMPORT CONST